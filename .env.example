# Example environment variables for deployment
# Copy this file to .env and fill in your actual values

# RPC URL for the network you want to deploy to
RPC_URL=https://rpc.ankr.com/0g_galileo_testnet_evm

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Token contract address (must be a valid Ethereum address)
TOKEN=0x0000000000000000000000000000000000000001

# Treasury address (must be a valid Ethereum address)
TREASURY=0xf39Fd6e51aad88F6F4ce6aB8827279cffFb92266
